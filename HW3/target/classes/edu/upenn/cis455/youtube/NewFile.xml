<?xml version="1.0" encoding="UTF-8"?>

<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

 <web-app>
   <display-name>Channel Subscription Servlets</display-name>
   <init-param>
     <param-name>cacheServer</param-name>
     <param-value>localhost</param-value>
   </init-param>
     
   <init-param>
     <param-name>cacheServerPort</param-name>
     <param-value>9001</param-value>
   </init-param>
    
   <servlet>
     <servlet-name>youtubesearch</servlet-name>
     <servlet-class>edu.upenn.cis455.servlet.YouTubeSearch</servlet-class> 
   </servlet>
    
   <servlet-mapping>
     <servlet-name>youtubesearch</servlet-name>
     <url-pattern>/youtubesearch</url-pattern>
   </servlet-mapping>  

   <!-- add any additional servlets and servlet mappings here -->

</web-app>
